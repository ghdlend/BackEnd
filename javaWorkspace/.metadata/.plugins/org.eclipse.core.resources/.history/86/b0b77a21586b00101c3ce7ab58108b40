package Package2;

public class Breack2 {

	public static void main(String[] args) {
		// 중첩 반복일 때 break 범위
		for(char upper = 'A'; upper <= 'Z'; upper++) {
			for(char lower = 'a'; lower <= 'z'; lower++) {
				System.out.println(upper + "-"+lower);
			}
		}
		System.out.println("프로그램 실행 종료");
	}

}
