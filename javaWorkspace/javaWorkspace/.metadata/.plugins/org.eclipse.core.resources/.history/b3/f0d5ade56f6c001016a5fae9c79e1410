package arrayType;

public class ArrayExer3 {

	public static void main(String[] args) {
		// 1~45까지 중복되지 않는 수 6개
		
		int lotto[] = new int[6];
		
		for(int i =1;i<lotto.length;i++) {
			int num = (int)Math.random()*45 +1;
		
			boolean duplicate = false;
			for(int j=0;j<i;j++) {
				if(lotto[i]==num) {
					duplicate = true;
					break;
				}
			}
			if(duplicate) {
				i--;
			}else {
				lotto[i]=num;
			}
		}
		
		System.out.println(Arrays.toString(lotto));
	}

}
